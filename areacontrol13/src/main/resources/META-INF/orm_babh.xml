<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings>
    <!-- Това е важно и не може да се махне с лека ръка
    По този начин се променя поведението на com.ib.indexui.db.dto.AdmUser което по подразбиране генерира ид със секуенс а тук искаме да се слага ръчно = ид-то от адм.структура.
    -->
    <entity class="com.ib.indexui.db.dto.AdmUser" name="AdmUser">
        <attributes>
            <id name="id" attribute-type="Integer">
                <column name="USER_ID" />
            </id>
        </attributes>
    </entity>

</entity-mappings>