							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="non-atmp">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="chmp-rapporteur_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="chmp-rapporteur" type="rdm:contact-details-type" minOccurs="0"/>
								<xs:element name="chmp-co-rapporteur_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="chmp-co-rapporteur" type="rdm:contact-details-type" minOccurs="0"/>
								<xs:element name="prac-rapporteur_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="prac-rapporteur" type="rdm:contact-details-type" minOccurs="0"/>
								<xs:element name="prac-co-rapporteur_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="prac-co-rapporteur" type="rdm:contact-details-type" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="atmp">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="cat-rapporteur_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="cat-rapporteur" type="rdm:contact-details-type" minOccurs="0"/>
								<xs:element name="cat-co-rapporteur_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="cat-co-rapporteur" type="rdm:contact-details-type" minOccurs="0"/>
								<xs:element name="chmp-coordinator_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="chmp-coordinator" type="rdm:contact-details-type" minOccurs="0"/>
								<xs:element name="chmp-co-coordinator_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="chmp-co-coordinator" type="rdm:contact-details-type" minOccurs="0"/>
								<xs:element name="prac-rapporteur_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="prac-rapporteur" type="rdm:contact-details-type" minOccurs="0"/>
								<xs:element name="prac-co-rapporteur_selected" type="rdm:yes-no" minOccurs="0"/>
								<xs:element name="prac-co-rapporteur" type="rdm:contact-details-type" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
